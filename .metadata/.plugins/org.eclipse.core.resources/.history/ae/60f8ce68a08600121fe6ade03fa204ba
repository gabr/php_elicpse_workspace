$(document).ready(function() {
	var height = $("#footer").offset().y - $("#menu-body").offset().y;
	console.log("footer y: "+$("#footer").offset().top);
	console.log("menu-body y: "+$("#menu-body").offset().top);
	console.log("wysokosc: "+height);
	$("#menu").css("height", height);
    $("#menu-body").css("height", height - 40);
});

